<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/news_nav_graph"
    app:startDestination="@id/headlinesFragment">
    <fragment
        android:id="@+id/articleFragment2"
        android:name="com.arjundabbe.jivanman.ui.fragments.HomeFragment"
        android:label="ArticleFragment" />
    <fragment
        android:id="@+id/favouritesFragment2"
        android:name="com.arjundabbe.jivanman.ui.fragments.SavedFragment"
        android:label="FavouritesFragment" >
        <action
            android:id="@+id/action_favouritesFragment2_to_articleFragment2"
            app:destination="@id/articleFragment2" />
    </fragment>
    <fragment
        android:id="@+id/headlinesFragment"
        android:name="com.arjundabbe.jivanman.ui.fragments.HeadlinesFragment"
        android:label="HeadlinesFragment" >
        <action
            android:id="@+id/action_headlinesFragment_to_articleFragment2"
            app:destination="@id/articleFragment2" />
    </fragment>
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.arjundabbe.jivanman.ui.fragments.SearchFragment"
        android:label="SearchFragment" >
        <action
            android:id="@+id/action_searchFragment_to_articleFragment2"
            app:destination="@id/articleFragment2" />
    </fragment>
</navigation>